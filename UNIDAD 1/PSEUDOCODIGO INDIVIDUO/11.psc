Algoritmo INTERCAMBIA_DOS_NUMERO
	Definir a Como Entero
	Definir b Como Entero
	Definir aux Como Entero
	Escribir "Teclea el valor de a"
	Leer a
	Escribir "a es" a
	Escribir "Teclea el valor de b"
	Leer b
	Escribir "b es"
	Escribir  "Vamos a intercambiar los valores"
	aux <- a
	a <- b
	b <- aux
	Escribir "a es " a
	Escribir "b es" b
FinAlgoritmo
